{
    "name": "programster/saml",
    "type": "library",
    "description": "A library to help with interfacing with the Jira API.",
    "keywords": ["Jira"],
    "homepage": "https://github.com/programster/package-jira-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "sdpagent@gmail.com",
            "homepage": "https://blog.programster.org/",
            "role": "Master of the universe."
        }
    ],
    "require": {
        "php": ">=8.0.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "symfony/http-client": "^5.3",
        "guzzlehttp/guzzle" : "^7.3",
        "guzzlehttp/psr7" : "^2.0",
        "programster/core-libs" : "2.1.*"
    },
    "autoload": {
        "psr-4": {
            "Programster\\Jira\\": "src",
            "Programster\\Jira\\Exceptions\\": "src/exceptions"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programster\\Jira\\Testing\\": "testing/",
            "Programster\\Jira\\Testing\tests\\": "testing/tests"
        }
    }
}
